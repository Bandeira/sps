journal = {Molecular Vision},
pages = {2597--2603},
publisher = {Molecular Vision},
title = {{Small ubiquitin-like modifier 4 (SUMO4) polymorphisms and Vogt-Koyanagi-Harada (VKH) syndrome in the Chinese Han population}},
