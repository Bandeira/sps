institution = {Molecular Structural Biology, Max Planck Institute of Biochemistry, 82152 Martinsried, Germany.},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
number = {49},
pages = {20992--20997},
publisher = {National Academy of Sciences},
title = {{Structure of the 26S proteasome from Schizosaccharomyces pombe at subnanometer resolution.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3000292\&tool=pmcentrez\&rendertype=abstract},
